{
  "editor.minimap.enabled": false,
  // "editor.fontFamily": "'Cascadia Code', 'monospace', monospace",
  // "editor.fontFamily": "'Fira Mono', 'monospace', monospace",
  // "editor.fontFamily": "'Ubuntu Mono', 'monospace', monospace",
  "editor.fontFamily": "'Geist Mono Light', 'monospace', monospace",
  "editor.fontSize": 13,
  //"terminal.integrated.fontFamily": "'Cascadia Code', 'monospace', monospace",
  //"terminal.integrated.fontFamily": "'Ubuntu Mono', 'monospace', monospace",
  "terminal.integrated.fontFamily": "'Geist Mono Light', 'monospace', monospace",
  "terminal.integrated.fontSize": 13,
  "workbench.colorTheme": "Userscape (rainglow)",
  /*
    "workbench.colorTheme": "Absent (rainglow)",
    "editor.tokenColorCustomizations": {
      "comments": {
        "foreground": "#616E76"
      }
    },*/
  "explorer.compactFolders": false,
  "workbench.tree.indent": 20,
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#4f93ff88", // 通常の選択色（単語を選択したときの背景）
    "editor.selectionHighlightBackground": "#4f93ff44", // 通常された単語と同じ単語の選択色（少し透明度をつけた一致色）
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "/usr/lib/jvm/msopenjdk-current"
    }
  ],
  "java.configuration.updateBuildConfiguration": "interactive",
  "java.inlayHints.parameterNames.enabled": "none",
  "window.zoomLevel": 1,
  "editor.codeLens": false
}